<html lang="en-gb">
<%- include('_head') -%>
<%- include('_header') -%>
<body>
<h1>Users</h1>
<ol>
    <% results.forEach(function(user) {%>
        <li>
            <a href="/users/<%= user.id %>"><%= user.name %></a> <br><br>
            <a href="<%= user.profileImageUrl %>" target="_blank">
                <img src="<%= user.profileImageUrl %>" height=50 width=50 alt="No image found"></a> <br>
        </li>
    <% }); %>
</ol>
<%if (next) { %>
    <a href="<%= next %>">Next</a>
<% } %>
<%if (previous) { %>
    <a href="<%= previous %>">Previous</a>
<% } %>
</body>
<%- include('_footer') -%>
</html>